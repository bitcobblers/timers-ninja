{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build -o dist/storybook",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.2",
    "@builder.io/qwik": "^1.5.4",
    "@builder.io/qwik-city": "^1.5.4",
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/addon-mdx-gfm": "^8.1.1",
    "@storybook/blocks": "^8.0.8",
    "@storybook/builder-vite": "^8.0.8",
    "@storybook/html": "^8.0.8",
    "@storybook/html-vite": "^8.1.1",
    "@types/eslint": "^8.56.10",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^0.34.7",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.4",
    "luxon": "^3.4.4",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.4",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "storybook": "^8.1.1",
    "storybook-framework-qwik": "^0.4.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.3.3",
    "undici": "^6.17.0",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@codemirror/theme-one-dark": "^6.1.2",
    "@qwikest/icons": "^0.0.13",
    "chevrotain": "^11.0.3",
    "codemirror": "^6.0.1"
  }
}
